@model InventoryManagement.Asset
<tr id="model-@Model.Id">
    <td>
        @Html.DisplayFor(modelItem => @Model.SerialNumber)
    </td>
    <td>
        @Html.DisplayFor(modelItem => @Model.AssetModel.AssetType.Type)
    </td>
    <td>
        @Html.DisplayFor(modelItem => @Model.AssetModel.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => @Model.AssetUser.fullName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => @Model.RoomNum)
    </td>
    <td>
        @Html.DisplayFor(modelItem => @Model.PurchaseDate)
    </td>
    <td class="text-center">
        <a href="@Url.Action("Reassign", "ManageAssets", new{id=@Model.Id})" class="btn btn-xs btn btn-success"><span class="glyphicon glyphicon-user"></span></a>
    </td>
    <td class="text-center">
        <a href="@Url.Action("_EditPartial", "ManageAssets", new { id=@Model.Id})" data-toggle="modal" data-target="#editModal" class="btn btn-info btn-xs">
            Edit
        </a>
    </td>
    <td class="text-center">
        <a href="@Url.Action("Delete", "ManageAssets", new { id=@Model.Id })" class="btn btn-danger btn-xs"><span class="glyphicon glyphicon-remove"></span></a>
    </td>
</tr>
